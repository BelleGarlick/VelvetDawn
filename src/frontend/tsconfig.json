{
  "compilerOptions": {
    "outDir": "./dist/",
    "types": ["node", "jest"],
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "api/*": ["api/*"],
      "models/*": ["models/*"],
      "ui/*": ["ui/*"]
    }
  }
}